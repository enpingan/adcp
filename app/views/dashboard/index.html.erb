        <%#= form_tag('/accounts/quick_mail', :id=>'form_quick_email') do |f| %>
        <div id="qmail_notice" style="padding: 2px 10px;color:palegoldenrod;height:18px;margin-top:3px;">
          <!-- <i class="fa fa-fw fa-exclamation-circle"></i>&nbsp;Invalid Email Address -->
          <!-- <i class="fa fa-fw fa-check-circle"></i>&nbsp;Emergency mail was sent successfully -->
        </div>
        <%= simple_form_for :quick_mail, :id=>'form_quick_email', :method=>'get', :html=>{:style=>'display:inline-block;width:100%'}, :remote=>true do |f| %>
          <div class="box-body">
            <div class="form-group">
              <!-- <input type="email" class="form-control tform-control" name="emailto" placeholder="Email to:"/> -->
              <%= f.email_field :emailto, :class=>'form-control tform-control', :required=>true, :autocomplete=>:off, :placeholder=>'Email to:' %>
            </div>
            <div class="form-group">
              <!-- <input type="text" class="form-control tform-control" name="subject" placeholder="Subject"/> -->
              <%= f.text_field :subject, :class=>'form-control tform-control', :required=>true, :autocomplete=>:off, :placeholder=>'Subject' %>
            </div>
            <div>
              <%= f.text_area :content, :class=>'form-control tform-control', :required=>true, :autocomplete=>:off, :placeholder=>'Message', :style=>'width: 100%; height: 125px; font-size: 14px; line-height: 18px; border: 1px solid #dddddd; padding: 10px;' %>
