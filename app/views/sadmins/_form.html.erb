<script>
  $('#select_sadmin_country').dropdown();
  $('#select_sadmin_type').dropdown();

  _fix();

  $('.form-control').keypress(function(e) {
    var code = (e.keyCode ? e.keyCode : e.which);
    if (code == 13) {
      if ($('#div_sadmin').css('display') == 'none')  {
        $('#div_sadmin').css('display', 'block');
        $('thml, body').animate({scrollTop: $(document).height()}, 1000);
        $('#password_hid').focus();
        $('#password_hid').val('');
        _fix();
        return false;
      } else {
        e.preventDefault();
        if ($('#password_hid').val() == "")  {
          $('thml, body').animate({scrollTop: $(document).height()}, 1000);
          $('#password_hid').focus();
          $('#password_hid').val('');
        }
        
        if ($('#new_sadmin').length) {
          show_loading();
          $('#new_sadmin').submit();
        }
        
        if ($('.edit_sadmin').length) {
          show_loading();
          $('.edit_sadmin').submit();  
        }
      }
    }
  });

  $('.edit_sadmin').keypress(function(e) {
    var code = (e.keyCode ? e.keyCode : e.which);
    // console.log(code);
    if (code == 13) {
      if ($('#div_cadmin').css('display') == 'none')  {
        $('thml, body').animate({scrollTop: $(document).height()}, 1000);
        $('#password_hid').focus();
        $('#password_hid').val('');
        return false;
      } else {
        e.preventDefault();
        show_loading();
        $('.edit_sadmin').submit();
      }
    }
  });

  $('#new_sadmin').keypress(function(e) {
    var code = (e.keyCode ? e.keyCode : e.which);
    // console.log(code);
    if (code == 13) {
      if ($('#div_cadmin').css('display') == 'none')  {
        $('thml, body').animate({scrollTop: $(document).height()}, 1000);
        $('#password_hid').focus();
        $('#password_hid').val('');
        return false;
      } else {
        e.preventDefault();
        show_loading();
        $('#new_sadmin').submit();
      }
    }
  });

  $('#select_sadmin_country div.text').bind("DOMSubtreeModified",function(){
    tmp = $(this).text();
    $('#sadmin_country').val(tmp);
  });

  if ( $('#btn_generator_pwd3').attr('disabled') == null ) {
    $('#btn_generator_pwd3').css('pointerEvents', 'auto');
  } else {
    $('#btn_generator_pwd3').css('pointerEvents', 'none');
  }

  $('.tnumber').keypress(function(e) {
    var code = (e.keyCode ? e.keyCode : e.which);

    if (code > 31 && code < 48 || code > 57 ) {
      return false;
    }

    return true;
  });

  $('.vchart').keypress(function(e) {
    var code = (e.keyCode ? e.keyCode : e.which);

    if ((code > 64 && code < 91) || (code > 96 && code < 123)) {
      return true;
    }

    return false;
  });

  $('.tnumber').on('change', function(e) {
    var tmp = $(this).val();
    var phone = tmp.replace(/(\d{3})(\d{4})(\d{4})/, '$1-$2-$3');

    $(this).val(phone);
  });

  hide_loading();

</script>