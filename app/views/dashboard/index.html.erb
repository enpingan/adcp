      <% if !@pcustomer1.nil? %>
        <div class="box box-warning" id="loading-example">
          <div class="box-header" style="background-color:rgba(0, 0, 0, 0.19);">
            <!-- tools box -->
            <div class="pull-right box-tools">
              <!-- <button class="btn btn-warning btn-sm btn-td" data-widget='collapse' data-toggle="tooltip" title="Collapse"><i class="fa fa-minus"></i></button>
              <button class="btn btn-warning btn-sm btn-td" data-widget='remove' data-toggle="tooltip" title="Remove"><i class="fa fa-times"></i></button> -->
            </div><!-- /. tools -->
            <i class="fa fa-fw fa-shopping-cart" style="color:rgb(228, 217, 113);"></i>

            <h3 class="box-title">Pending Customer</h3>

          </div><!-- /.box-header -->
          <div class="box-body no-padding">
            <br/>
            <div class="row">
              <div class="col-lg-12">
                <div class="col-lg-4">
                  <div class="chart" id="bar-chart" style="text-align:center;">
                    <%#= image_tag('server1.png', size: "80") %>
                    <i class="fa fa-fw fa-user" style="color:rgb(228, 217, 113);font-size:72px;"></i>                    
                  </div>
                </div>
                <div class="col-lg-8">
                  <div class="clearfix">
                    <span class="pull-left" style="font-size:16px;">&nbsp;&nbsp;Username: </span>
                    <span class="pull-right" style="font-size:16px;color:rgb(228, 217, 113);;">&nbsp;&nbsp;<i class="fa fa-fw fa-user"></i><%= @pcustomer1.username %></small></span>
                    <br/><br/>
                    <span class="pull-left" style="font-size:16px;">&nbsp;&nbsp;Personal Email: </span>
                    <span class="pull-right" style="font-size:16px;color:rgb(228, 217, 113);;"><i class="fa fa-fw fa-envelope" style="color:rgb(228, 217, 113);"></i>&nbsp;<%= @pcustomer1.pemail %></small></span>
                  </div>
                </div>
              </div>
              <div class="col-lg-12">
                <br/>
                <div class="pad">
                  <!-- Progress bars -->
                  <div class="row">
                    <div class="col-lg-6">
                      <div class="clearfix">
                        <span class="pull-left"><i class="fa fa-fw fa-font" style="color:rgb(228, 217, 113);"></i>&nbsp;First Name: </span>
                        <small class="pull-right"><%= @pcustomer1.first_name %></small>
                      </div>

                      <div class="clearfix">
                        <span class="pull-left"><i class="fa fa-fw fa-bold" style="color:rgb(228, 217, 113);"></i>&nbsp;Last Name: </span>
                        <small class="pull-right"><%= @pcustomer1.last_name %></small>
                      </div>

                      <div class="clearfix">
                        <span class="pull-left"><i class="fa fa-fw fa-building-o" style="color:rgb(228, 217, 113);"></i>&nbsp;City: </span>
                        <small class="pull-right"><%= @pcustomer1.city %></small>
                      </div>
                      <div class="clearfix">
                        <span class="pull-left"><i class="fa fa-fw fa-home" style="color:rgb(228, 217, 113);"></i>&nbsp;State: </span>
                        <small class="pull-right"><%= @pcustomer1.state %></small>
                      </div>

                      <div class="clearfix">
                        <span class="pull-left"><i class="fa fa-fw fa-flag" style="color:rgb(228, 217, 113);"></i>&nbsp;Country: </span>
                        <small class="pull-right"><%= raw(get_countryname(@pcustomer1.country)) %></small>
                      </div>

                      <div class="clearfix">
                        <span class="pull-left"><i class="fa fa-fw fa-phone" style="color:rgb(228, 217, 113);"></i>&nbsp;Number: </span>
                        <small class="pull-right"><%= @pcustomer1.number %></small>
                      </div>

                    </div>
                    <div class="col-lg-6">
                      <div class="clearfix">
                        <span class="pull-left">Create Ticket </span>
                        <small class="pull-right"><%= raw(get_checkicon(@pcustomer1.is_rticket)) %></small>
                      </div>

                      <div class="clearfix">
                        <span class="pull-left">Modify Ticket</span>
                        <small class="pull-right"><%= raw(get_checkicon(@pcustomer1.is_wticket)) %></small>
                      </div>

                      <div class="clearfix">
                        <span class="pull-left">Delete Ticket</span>
                        <small class="pull-right"><%= raw(get_checkicon(@pcustomer1.is_dticket)) %></small>
                      </div>

                      <div class="clearfix">
                        <span class="pull-left">Create Device </span>
                        <small class="pull-right"><%= raw(get_checkicon(@pcustomer1.is_cdevice)) %></small>
                      </div>

                      <div class="clearfix">
                        <span class="pull-left">Modify Device</span>
                        <small class="pull-right"><%= raw(get_checkicon(@pcustomer1.is_mdevice)) %></small>
                      </div>

                      <div class="clearfix">
                        <span class="pull-left">Delete Device</span>
                        <small class="pull-right"><%= raw(get_checkicon(@pcustomer1.is_rdevice)) %></small>
                      </div>
                    </div>
                  </div>
                  <br/>
                </div><!-- /.pad -->
                <div class="box-footer clearfix" style="text-align:right;">
                  <% if hp_customer(current_user.username) || is_adminuser(current_user.username) %>
                    <%= link_to pcustomer_path(@pcustomer1), :class=>'btn btn-warning btn-sm btn-td a_loading' do %>
                      Accept&nbsp;<i class="fa fa-fw fa-eye"></i>
                    <% end %>
                  <% end %>
                </div>
              </div><!-- /.col -->
            </div><!-- /.row - inside box -->
          </div><!-- /.box-body -->
        </div>
      <% end %>
    </div><!-- ./col -->
  </div><!-- /.row -->
</section>

</div>

<script>
  $('#li_dashboard').attr('class', 'active');

  hide_loading();
</script>