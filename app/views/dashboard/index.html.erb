            </div>
          </div>
          <div class="box-footer clearfix">
            <%= button_tag(type:"submit", :class=>'pull-right btn btn-info btn-td', :id=>'send_qmail') do %>
              Send <i class="fa fa-arrow-circle-right"></i>
            <% end %>
          </div>
        <% end %>
