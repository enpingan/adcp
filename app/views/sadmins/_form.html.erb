  <%= form_for(@sadmin) do |f| %>
    <% if @sadmin.errors.any? %>
      <div id="error_explanation">
        <h2><%= pluralize(@sadmin.errors.count, "error") %> prohibited this sadmin from being saved:</h2>

        <ul>
        <% @sadmin.errors.full_messages.each do |message| %>
          <li><%= message %></li>
        <% end %>
        </ul>
      </div>
    <% end %>
    <%= f.hidden_field :id, :value=>@sadmin.id %>

    <div class="small-box bg-green" style="margin-bottom:0px;">
      <div class="inner">
        <h3>
          Suspended Admins
        </h3>
        <p>
          Account ID: #&nbsp;<%= @sadmin.id%>
        </p>
      </div>
      <div class="icon">
        <i class="fa fa-fw fa-user"></i>
      </div>
      <a href="/sadmins" class="small-box-footer">
        <i class="fa fa-arrow-circle-left"></i>&nbsp;Back
      </a>
    </div>
    <div class="box box-danger" style="border:none">
      <div class="box-header" style="background-color:rgba(0, 0, 0, 0.19);padding:5px 0;">
        <div class="pull-right box-tools">
          
        </div>
        <i class="fa fa-fw fa-home" style="color:rgb(228, 217, 113);"></i>

        <h3 class="box-title">Contact Info</h3>
      </div><!-- /.box-header -->
      <div class="box-body row" style="padding: 20px 26px;">
        <div class="col-lg-6">
          <div class="field">
            <label><span style="color:red">*</span>&nbsp;First name:</label><br/>
            <div class="inner-addon left-addon">
              <i class="fa fa-fw fa-font" style="color:#5bc0de;font-size:18px;margin:-2px 0;"></i>
              <%#= f.text_field :email, autofocus: true, :required=>true, :placeholder=>'SSL Name', :autocomplete=>:off'%>
              <%= f.text_field :first_name, :class=>'form-control tform-control vchart', :required=>true, :style=>'padding-left:35px;', :autocomplete=>:off %>
            </div>
          </div>
          <div class="field">
            <label><span style="color:red">*</span>&nbsp;Last name:</label><br/>
            <div class="inner-addon left-addon">
              <i class="fa fa-fw fa-bold" style="color:#5bc0de;font-size:18px;margin:-2px 0;"></i>
              <%= f.text_field :last_name, :class=>'form-control tform-control vchart', :required=>true, :style=>'padding-left:35px;', :autocomplete=>:off %>
            </div>
          </div>
        </div>

        <div class="col-lg-6">
          <div class="field">
            <label><span style="color:red">*</span>&nbsp;Personal Email Address:</label><br/>
            <div class="inner-addon left-addon">
              <i class="fa fa-fw fa-envelope" style="color:#5bc0de;font-size:14px;margin:-2px 0;"></i>
              <%= f.text_field :pemail, :class=>'form-control tform-control', :required=>true, :style=>'padding-left:35px;', :autocomplete=>:off %>
            </div>
          </div>

          <div class="field">
            <label><span style="color:red">*</span>&nbsp;Mobile Number:</label><br/>
            <div class="inner-addon left-addon">
              <i class="fa fa-fw fa-phone" style="color:#5bc0de;font-size:18px;margin:-2px 0;"></i>
              <%= f.text_field :number, :class=>'form-control tform-control tnumber', :required=>true, :style=>'padding-left:35px;', minlength:'11', maxlength: '11', :autocomplete=>:off %>
            </div>
          </div>
        </div>

        <div class="col-lg-6">
          <div class="field">
            <label><span style="color:red">*</span>&nbsp;City:</label><br/>
            <div class="inner-addon left-addon">
              <i class="fa fa-fw fa-building-o" style="color:#5bc0de;font-size:18px;margin:-2px 0;"></i>
              <%#= f.text_field :email, autofocus: true, :required=>true, :placeholder=>'SSL Name', :autocomplete=>:off'%>
              <%= f.text_field :city, :class=>'form-control tform-control vchart', :required=>true, :style=>'padding-left:35px;', :autocomplete=>:off %>
            </div>
          </div>
          <div class="field">
            <label><span style="color:red">*</span>&nbsp;State:</label><br/>
            <div class="inner-addon left-addon">
              <i class="fa fa-fw fa-home" style="color:#5bc0de;font-size:18px;margin:-2px 0;"></i>
              <%= f.text_field :state, :class=>'form-control tform-control vchart', :required=>true, :style=>'padding-left:35px;', :autocomplete=>:off %>
            </div>
          </div>
        </div>

        <div class="col-lg-6">
          <label><span style="color:red">*</span>&nbsp;Country:</label><br/>
          <%= f.hidden_field :country %>
